/* components/my-search/my-serch.wxss */
/* 搜索栏 */
.search-box{
  box-sizing: border-box;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 2;
  width: 100%;
  height: 100rpx;
  background-color:#c00000;
  padding: 20rpx 14rpx;
}
/* 搜索栏里的按钮 */
.search-box .search-button{
  width: 100%;
  height: 72rpx;
  font-size: 26rpx;
  color: #000000;
  border-radius: 30rpx;
}